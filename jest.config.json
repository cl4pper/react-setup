{
  "testRegex": "((\\.|/*.)(test))\\.tsx?$",
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
	"src/{components,containers}/**/*.{js,jsx,ts,tsx}",
	"!src/{components,containers}/index.{js,ts}"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy",
    "@components": "<rootDir>/src/components"
  }
}