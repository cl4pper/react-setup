{
  "testRegex": "((\\.|/*.)(test))\\.tsx?$",
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
	"src/{components,containers}/**/*.tsx",
	"!src/{components,containers}/index.{js,jsx,ts}"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy",
    "@components": "<rootDir>/src/components"
  }
}